/**
 * And gate:
 * if (a and b) out = 1, else out = 0
 */
CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=not_a);
    Not(in=b, out=not_b);
    Nand(a=not_a, b=not_b, out=out);
}
